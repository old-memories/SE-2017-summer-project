<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.Exchange" table="exchange">
        <id name="exchangeID" column="exchange_id">
            <generator class="native"/>
        </id>
        <property name="userID" column="user_id" />
        <property name="wantedID" column="wanted_id" />
        <property name="hadID" column="had_id" />
        <property name="applyDate" column="applydate" type="timestamp" />
    </class>
</hibernate-mapping>